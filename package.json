{
  "name": "fhirtypes",
  "version": "1.0.28",
  "description": "FHIR Types",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertoAraneda/fhir-types"
  },
  "bugs": {
    "url": "https://github.com/robertoAraneda/fhir-types/issues",
    "email": "robaraneda@gmail.com"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "npm run lint && npm run test",
    "pre-push": "npm run release",
    "build": "tsc -p tsconfig.enum.json && tsc -p tsconfig.type.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags",
    "release": "standard-version && git push --follow-tags --no-verify",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.0.1"
  },
  "keywords": [
    "FHIR",
    "Types",
    "Validator",
    "Resource"
  ],
  "author": "Roberto Araneda",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md",
    "package.json",
    "LICENSE.md"
  ]
}
