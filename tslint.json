{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-shadowed-variable": false,
    "no-empty-interface": false,
    "no-namespace": false,
    "@typescript-eslint/member-ordering": [
      "error",
      { "default": ["signature", "method", "constructor", "field"] }
    ]
  }
}
